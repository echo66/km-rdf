@prefix vamp: <http://vamp-plugins.org/plugin/>.
@prefix qvp: <http://vamp-plugins.org/plugin/qm-vamp-plugins/>.
@prefix evp: <http://vamp-plugins.org/plugin/vamp-example-plugins/>.
@prefix vo : <http://vamp-plugins/onto/>.
@prefix : <>.


vamp:qm-vamp-plugins 
	a  vo:PluginLibrary;
	rdfs:label """
		A set of VAMP plugins developed
		in the C4DM, Queen Mary, University
		of London
	""";
	vo:plugin qvp:qm-chromagram;
	vo:plugin qvp:qm-constantq;
	vo:plugin qvp:qm-keydetector;
	vo:plugin qvp:qm-onsetdetector;
	vo:plugin qvp:qm-tempotracker;
	vo:plugin qvp:qm-tonalchange;
	vo:binary [
		a vo:BinaryLibrary;
		vo:platform vo:x86;
		vo:os vo:linux_32;
		vo:download <http://www.elec.qmul.ac.uk/digitalmusic/downloads/qm-vamp-plugins-1.2-i686-linux.tar.gz>;
		];
	vo:binary [
		a vo:BinaryLibrary;
		vo:platform vo:x86;
		vo:os vo:linux_64;
		vo:download <http://moustaki.org/resources/libqm-vamp-plugins.so>;
		];
	.

vamp:vamp-example-plugins 
	a vo:PluginLibrary;
	rdfs:label """
		A set of examples VAMP plugins
		""";
	vo:plugin evp:amplitudefollower;
	vo:plugin evp:percussiononsets;
	vo:plugin evp:spectralcentroid;
	vo:plugin evp:zerocrossing;
	.


qvp:qm-chromagram
	a vo:Plugin;
	.
qvp:qm-constantq
	a vo:Plugin;
	.
qvp:qm-keydetector
	a vo:Plugin;
	.
qvp:qm-onsetdetector
	a vo:Plugin;
	.
qvp:qm-tempotracker
	a vo:Plugin;
	.
qvp:qm-tonalchange;
	a vo:Plugin;
	.

evp:amplitudefollower
	a vo:Plugin;
	.
evp:percussiononsets
	a vo:Plugin;
	.
evp:spectralcentroid
	a vo:Plugin;
	.
evp:zerocrosssing
	a vo:Plugin;
	.


