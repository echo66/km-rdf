@prefix vamp: <http://purl.org/ontology/vamp/>.
@prefix qvp: <http://vamp-plugins.org/plugin/qm-vamp-plugins/>.
@prefix evp: <http://vamp-plugins.org/plugin/vamp-example-plugins/>.
@prefix vp: <http://vamp-plugins.org/plugin/>.
@prefix : <>.

#This file provides rdf-descriptions of the existing Vamp Plugin Libraries.

vp:qm-vamp-plugins 
	a  vamp:PluginLibrary;
	rdfs:label """
		A set of VAMP plugins developed
		in the C4DM, Queen Mary, University
		of London
	""";
	vamp:available_plugin qvp:qm-chromagram;
	vamp:available_plugin qvp:qm-constantq;
	vamp:available_plugin qvp:qm-keydetector;
	vamp:available_plugin qvp:qm-onsetdetector;
	vamp:available_plugin qvp:qm-tempotracker;
	vamp:available_plugin qvp:qm-tonalchange;
	vamp:available_plugin qvp:qm-mfcc;
	vamp:available_plugin qvp:qm-segmenter;
	vamp:available_plugin qvp:qm-similarity;
	vamp:binary [
		a vamp:BinaryLibrary;
		vamp:platform vamp:x86;
		vamp:os vamp:linux_32;
		vamp:download <http://www.elec.qmul.ac.uk/digitalmusic/downloads/qm-vamp-plugins-1.2-i686-linux.tar.gz>;
		];
	vamp:binary [
		a vamp:BinaryLibrary;
		vamp:platform vamp:x86;
		vamp:os vamp:linux_64;
		vamp:download <http://moustaki.org/resources/libqm-vamp-plugins.so>;
		];
	.

vp:vamp-example-plugins 
	a vamp:PluginLibrary;
	rdfs:label """
		A set of examples VAMP plugins
		""";
	vamp:available_plugin evp:amplitudefollower;
	vamp:available_plugin evp:percussiononsets;
	vamp:available_plugin evp:spectralcentroid;
	vamp:available_plugin evp:zerocrossing;
	.


