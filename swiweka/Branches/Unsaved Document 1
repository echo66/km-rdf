wkpl_create_emptySet(Name, Capacity, Instances):-
	wkpl_create_FastVector_empty(FastVector),
	wkpl_new_argsType_array(3, Args),
	wkpl_add_type_to_args('java.lang.String', Args, 0),
	jpl_class_to_classname(FastVector, FV),
	wkpl_add_type_to_args(FV, Args, 1),
	jpl_call('java.lang.reflect.Array', set, [Args, 1, FV], _),
	jpl_type_to_class(int, ClassInt),
	jpl_call('java.lang.reflect.Array', set, [Args, 2, ClassInt], _),
	jpl_datums_to_array([Name, FastVector, Capacity], Values),
	wkpl_getObject_arg('weka.core.Instances', [Args], [Values], Instances).
